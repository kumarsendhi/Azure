var joinMail={initialize:function(){joinMail.attachEvents();joinMail.initializeValidation()},initializeValidation:function(){$("#joinNewsLetterForm").validate({rules:{name:{required:!0},lastName:{required:!0},emailAddress:{required:!0,email:!0},CaptchaInputText:{required:!0}},messages:{name:{required:"Please Enter First Name"},lastName:{required:"Please Enter Last Name"},emailAddress:{required:"Email is required.",email:"Please enter a valid email."},CaptchaInputText:{required:"Please enter captcha."}}})},attachEvents:function(){$("body").off("click",".registerButton");$("body").on("click",".registerButton",function(){$("#joinNewsLetterForm").validate();$("#joinNewsLetterForm").valid()&&$("#joinNewsLetterForm").submit()})},onSucess:function(n){n.Status==!1&&$("#captchaError").show()}}