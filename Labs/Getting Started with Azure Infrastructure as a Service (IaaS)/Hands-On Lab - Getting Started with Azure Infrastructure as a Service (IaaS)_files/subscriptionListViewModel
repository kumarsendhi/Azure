var subscriptionListViewModel={SubscriptionNames:ko.observableArray([]),currentSubscription:ko.observable(),currentSubscriptionId:ko.observable(),hasValidSubscription:ko.observable(!1),loadSubscriptionNameList:function(){var n=this;ajaxRequest.makeAPIRequest("/Subscription/GetCurrentSubscriptions","GET",null,function(t){if(n.SubscriptionNames(t),t.length>0){var i=$.getCookie("CurrentSubscriptionToken");i||(i=t[0]);subscriptionListViewModel.currentSubscription(i.Subscription);opsgility.changeSubscriptionDropDownWidth()}else $.deleteCookie("CurrentSubscriptionToken")})},selectionChanged:function(){var n=$("#mySubscriptions").val(),t=$.getCookie("CurrentSubscriptionToken");n!=t&&($.deleteCookie("OpsgilityAccessToken"),$.setCookie("CurrentSubscriptionToken",n,1));opsgility.changeSubscriptionDropDownWidth()},loadSubscriptionId:function(){var n=this;ajaxRequest.makeAPIRequest("/Subscription/GetCurrentSubscriptionId","GET",null,function(t){t!=0&&(n.currentSubscriptionId(t),t>0&&subscriptionListViewModel.hasValidSubscription(!0))},null,null,!1,!1)},attachEvents:function(){$("body").off("click",".homecertificate");$("body").on("click",".homecertificate",function(){var n=$(this).attr("certificateId"),t=opsgility.getBaseURL();window.location.href=t+"/courses/bypath/"+n})}}